{
  "name": "auroville-community-platform",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "vite",
    "dev:local": "NODE_ENV=development vite --config vite.config.dev.ts",
    "build:dev": "tsc && NODE_ENV=development vite build --config vite.config.dev.ts",
    "build:prod": "tsc && NODE_ENV=production vite build --config vite.config.prod.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start:server": "cd server && npm start",
    "start:all": "npm run dev:local",
    "typecheck": "tsc --noEmit",
    "verify:build": "npm run typecheck && npm run lint",
    "predeploy": "npm run verify:build && npm run build:prod"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@radix-ui/react-dropdown-menu": "^2.1.3",
    "@tanstack/react-query": "^5.62.7",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "axios": "^1.6.2",
    "date-fns": "^2.30.0",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-router-dom": "^7.0.2",
    "recharts": "^2.10.3",
    "tailwind-merge": "^2.5.5",
    "typescript": "^5.2.2",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.0.0",
    "@types/react-query": "^1.2.9",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.54.0",
    "eslint-plugin-react-hooks": "^4.6.0", 
    "eslint-plugin-react-refresh": "^0.4.4",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "vite": "^5.0.0",
    "terser": "^5.24.0"
  }
}
